{
  "author": "jamesfchen",
  "description": "该配置文件给idea plugin插件和gradle plugin读取,idea plugin通过该文件可以实现管理想要编译的模块，gradle plugin则可以根据这些模块信息生成快速模块",
  "buildVariants": [
    "debug",
    "release",
    "inhouse"
  ],
  "groupId": "io.github.jamesfchen",
  "allModules": [
    {
      "simpleName": "uamp-automotive",
      "format": "app",
      "group": "uamp",
      "sourcePath": ":components:uamp-module:automotive"
    },
    {
      "simpleName": "uamp-main",
      "format": "app",
      "group": "uamp",
      "sourcePath": ":components:uamp-module:main"
    },
    {
      "simpleName": "uamp-uamp",
      "format": "bundle",
      "group": "uamp",
      "sourcePath": ":components:uamp-module:uamp"
    },
    {
      "simpleName": "update-module",
      "format": "bundle",
      "group": "update",
      "sourcePath": ":components:update-module"
    },
    {
      "simpleName": "codescanner-module",
      "format": "bundle",
      "group": "codescanner",
      "sourcePath": ":components:codescanner-module"
    },
    {
      "simpleName": "im-module",
      "format": "bundle",
      "group": "im",
      "sourcePath": ":components:im-module"
    },
    {
      "simpleName": "account-module",
      "format": "bundle",
      "group": "account",
      "sourcePath": ":components:account-module"
    },
    {
      "simpleName": "main-module",
      "format": "bundle",
      "group": "main",
      "sourcePath": ":components:main-module"
    },
    {
      "simpleName": "hotel-bundle1",
      "format": "bundle",
      "group": "hotel",
      "sourcePath": ":components:hotel-module:bundle1"
    },
    {
      "simpleName": "hotel-bundle2",
      "format": "bundle",
      "group": "hotel",
      "sourcePath": ":components:hotel-module:bundle2"
    },
    {
      "simpleName": "hotel-export",
      "format": "bundle_foundation",
      "group": "hotel",
      "sourcePath": ":components:hotel-module:export"
    },
    {
      "simpleName": "hotel-foundation",
      "format": "bundle_foundation",
      "group": "hotel",
      "sourcePath": ":components:hotel-module:foundation"
    },
    {
      "simpleName": "feeds-module",
      "format": "bundle",
      "group": "feeds",
      "sourcePath": ":components:feeds-module"
    },
    {
      "simpleName": "framework-base",
      "format": "framework_foundation",
      "group": "fwk",
      "sourcePath": ":base"
    },
    {
      "simpleName": "framework-lint-rules",
      "format": "framework_foundation",
      "group": "fwk",
      "sourcePath": ":lint-rules"
    },
    {
      "simpleName": "livestreaming-module",
      "format": "bundle",
      "group": "livestreaming",
      "sourcePath": ":components:livestreaming-module"
    }
  ]
}
